<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ComplementoMain">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="1dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageViewoct"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:onClick="Octubre"
                app:srcCompat="@drawable/complemento500x500" />


            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:text="DESCRIPCIÓN GENERAL"
                android:textColor="#009688"
                android:textSize="20sp"
                tools:layout_editor_absoluteX="164dp"
                tools:layout_editor_absoluteY="56dp" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:text="Complemento es un fertilizante mineral sólido, a base de una adecuada concentración de Potasio, Nitrógeno y Boro, es formulado especialmente para cumplir la función de traslocación de azúcares y almidones a los órganos de cosecha, obteniendo como resultado una mejor calidad de frutos (COLOR, CALIBRE, GRANOS BRIX). Su empleo hace parte de una nutrición complementaria al NPK tradicional, y por su aporte balanceado de Nitrógeno, Potasio y Boro, lo hace ideal para aplicarlo en cualquier tipo de cultivo. "
                android:textSize="20sp"
                tools:layout_editor_absoluteX="164dp"
                tools:layout_editor_absoluteY="56dp" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:text="VENTAJAS"
                android:textColor="#009688"
                android:textSize="20sp"
                tools:layout_editor_absoluteX="164dp"
                tools:layout_editor_absoluteY="56dp" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:text="Activa las enzimas en las plantas esenciales para la producción de ATP (Adeosina Trifosfato) fuente principal de energía para procesos químicos en las células de las plantas. "
                android:textColor="#000000"
                android:textSize="20sp"
                tools:layout_editor_absoluteX="164dp"
                tools:layout_editor_absoluteY="56dp" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:text="Evita la marchitez prematura en las plantas."
                android:textSize="20sp"
                tools:layout_editor_absoluteX="164dp"
                tools:layout_editor_absoluteY="56dp" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:text="Regula la apertura y cierre de las estomas, y por lo tanto regula la absorción de Co2 "
                android:textColor="#000000"
                android:textSize="20sp"
                tools:layout_editor_absoluteX="164dp"
                tools:layout_editor_absoluteY="56dp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:text="Mejora la tolerancia de la planta al estrés hídrico. "
                android:textSize="20sp"
                tools:layout_editor_absoluteX="164dp"
                tools:layout_editor_absoluteY="56dp" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:text="Mejora la calidad de los frutos."
                android:textColor="#000000"
                android:textSize="20sp"
                tools:layout_editor_absoluteX="164dp"
                tools:layout_editor_absoluteY="56dp" />


            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:text="PRECAUCIONES"
                android:textColor="#009688"
                android:textSize="20sp"
                tools:layout_editor_absoluteX="164dp"
                tools:layout_editor_absoluteY="56dp" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:text="Complemento es compatible con la mayoría de los productos fitosanitarios, sin embargo se recomienda hacer pequeñas pruebas de compatibilidad. "
                android:textColor="#000000"
                android:textSize="20sp"
                tools:layout_editor_absoluteX="164dp"
                tools:layout_editor_absoluteY="56dp" />
            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:text="No es un producto inflamable. En caso de incendio en el entorno esta permitidos todos los agentes extintores. "
                android:textSize="20sp"
                tools:layout_editor_absoluteX="164dp"
                tools:layout_editor_absoluteY="56dp" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:text="Almacenar en ambientes secos, preferiblemente sobre estibas y separado de sustancias combustibles; evitar el exceso de calor. "
                android:textColor="#000000"
                android:textSize="20sp"
                tools:layout_editor_absoluteX="164dp"
                tools:layout_editor_absoluteY="56dp" />
            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:text="No se ha detectado fitotoxicidad en los cultivos si es utilizado bajo las indicaciones del técnico, sin embargo, por estar la aplicación fuera de nuestro control declinamos toda responsabilidad por daños eventuales por su uso. "
                android:textSize="20sp"
                tools:layout_editor_absoluteX="164dp"
                tools:layout_editor_absoluteY="56dp" />


        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>