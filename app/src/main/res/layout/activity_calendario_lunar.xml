<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CalendarioLunar">

  <ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    tools:layout_editor_absoluteX="1dp">

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:orientation="vertical">

    <ImageView
        android:id="@+id/imageViewoct"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:onClick="Octubre"
        app:srcCompat="@drawable/octubre300x300" />

    <ImageView
        android:id="@+id/imageViewnov"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:onClick="Noviembre"
        app:srcCompat="@drawable/noviembre300x300" />

    <ImageView
        android:id="@+id/imageViewdic"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:onClick="Diciembre"
        app:srcCompat="@drawable/diciembre300x300" />

    <ImageView
        android:id="@+id/imageViewene"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:onClick="Enero"
        app:srcCompat="@drawable/enero300x300" />

    <ImageView
        android:id="@+id/imageViewfeb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:onClick="Febrero"
        app:srcCompat="@drawable/febrero300x300" />

    <ImageView
        android:id="@+id/imageViewmar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:onClick="Marzo"
        app:srcCompat="@drawable/marzo300x300" />

    <ImageView
        android:id="@+id/imageViewabr"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:onClick="Abril"
        app:srcCompat="@drawable/abril300x300" />

    <ImageView
        android:id="@+id/imageViewmay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:onClick="Mayo"
        app:srcCompat="@drawable/mayo300x300" />

    <ImageView
        android:id="@+id/imageViewjun"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="10dp"
        android:onClick="Junio"
        app:srcCompat="@drawable/junio300x300" />
    <ImageView
        android:id="@+id/imageViewjul"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="10dp"
        android:onClick="Julio"
        app:srcCompat="@drawable/julio300x300" />
    <ImageView
        android:id="@+id/imageViewago"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="10dp"
        android:onClick="Agosto"
        app:srcCompat="@drawable/agosto300x300" />
    <ImageView
        android:id="@+id/imageViewsep"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="10dp"
        android:onClick="Septiembre"
        app:srcCompat="@drawable/septiembre300x300" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fuente: Calendario Agricola Lunar 2023"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="164dp"
        tools:layout_editor_absoluteY="56dp" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ing. Santiago Bakach"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="164dp"
        tools:layout_editor_absoluteY="56dp" />

  </LinearLayout>
</ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>